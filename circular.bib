@article{Kucera2016,
title = "Circular units of an abelian field ramified at three primes ",
journal = "Journal of Number Theory ",
volume = "163",
number = "",
pages = "296 - 315",
year = "2016",
note = "",
issn = "0022-314X",
doi = "https://doi.org/10.1016/j.jnt.2015.11.023",
url = "http://www.sciencedirect.com/science/article/pii/S0022314X16000287",
author = "Radan Kučera and Azar Salami",
keywords = "Abelian field",
keywords = "Circular (cyclotomic) units",
keywords = "Ennola relation ",
abstract = "Abstract This paper constructs a basis and gives a presentation of Sinnott's group of circular units for a real abelian field k ramified at three primes whose genus field K in the narrow sense has cyclic relative Galois group Gal ( K / k ) . It is shown that, for this type of fields, the quotient of the module of all relations satisfied by circular units by the submodule generated by all norm relations is a cyclic module generated by an Ennola relation. "

}

